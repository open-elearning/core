<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<version>1.0</version>	
	<properties>	
		<type>object</type>
		<id>bigCheckGreen</id>
		<title>CheckGreen</title>
		<category>content</category>
	</properties>
	
	<embeddedFiles>
		<file>bigCheckGreenScreen.png</file>
		<file>bigCheckGreenScreenNo.png</file>
	</embeddedFiles>
	
	<display>
		<screenTextMapping>
			<svg width="100" height="60" >
				<rect ref="1" type="image" defautsrc="bigCheckGreenScreenNo.png" src="{ref}" x="0" y="0" width="100" height="60" ></rect>
			</svg>
		</screenTextMapping>
		<defaultText>0|bigCheckGreenScreenNo.png|0|0|</defaultText>
		<defaultText></defaultText>
		<width>100</width>
		<height>60</height>
		<sizeText>30</sizeText>
		<colorText>black</colorText>
		<textAlign>center</textAlign>
		<behavior>center</behavior>
	</display>
	
	<options>
		<resize>0</resize>
		<changeImage>0</changeImage>
		<changeText>0</changeText>
		<conditionalObject>1</conditionalObject>
		<haveScore>1</haveScore>
		<docTypeStrict>1</docTypeStrict>
	</options>
	
	
</plugin>